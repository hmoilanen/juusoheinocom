    "vue": "^2.6.10",
    "vue-head": "^2.2.0",
    "vue-router": "^3.1.3",
    "vue-slick-carousel": "^1.0.6",
    "vue2-dropzone": "^3.6.0",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-e2e-cypress": "^4.1.0",
    "@vue/cli-plugin-pwa": "^4.1.0",
